{
  "name": "koa-mock-switch",
  "version": "1.0.14",
  "description": "easy way to swich mock status without changing source mock data",
  "main": "lib/index.js",
  "scripts": {
    "mock": "nodemon -q --inspect ./demo/mockServer/server.js",
    "demo": "webpack-dev-server --open --config webpack.demo.config",
    "lint": "eslint --ext .js ./",
    "release": "np --yolo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeLittlePrince/koa-mock-switch.git"
  },
  "keywords": [
    "mock-switch"
  ],
  "author": "https://github.com/CodeLittlePrince",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CodeLittlePrince/koa-mock-switch/issues"
  },
  "homepage": "https://github.com/CodeLittlePrince/koa-mock-switch#readme",
  "devDependencies": {
    "eslint": "^7.8.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "mockjs": "^1.0.1-beta3",
    "nodemon": "^2.0.4",
    "np": "^6.5.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "boom": "^7.3.0",
    "chalk": "^2.4.1",
    "koa": "^2.6.2",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0"
  }
